{{ ansible_managed | comment }}

/var/log/tomcat/localhost.log
/var/log/tomcat/localhost_access_log_openam.log
/var/log/tomcat/catalina.log {
    su tomcat tomcat
    create 0644 tomcat tomcat
    copytruncate
    daily
    rotate 14
    compress
    missingok
    dateext
    dateformat -%Y%m%d.%s
}
